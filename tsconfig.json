{
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": "./src",
    "paths": {
      "@defikit/rocketlaunch_sdk/clients/*": ["./clients/*"],
      "@defikit/rocketlaunch_sdk/constants/*": ["./constants/*"],
      "@defikit/rocketlaunch_sdk/types/*": ["./types/*"]
    },
    "moduleResolution": "node",
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}